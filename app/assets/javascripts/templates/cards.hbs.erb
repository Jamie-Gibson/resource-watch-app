<div class="row -expanded">
  {{#if cards.length}}
    {{#each cards}}
      {{#if ../grid}}
        {{#if ../gridClasses}}
          <div class="{{../gridClasses}}">
        {{else}}
          <div class="col -xs-12 -sm-6 -md-4 -lg-4">
        {{/if}}
      {{else}}
        <div class="col -xs-12">
      {{/if}}
          <div class="js-card"></div>
        </div>
    {{/each}}
  {{else}}
    <div class="col -xs-12">
      <p><%= I18n.t('not_dataset_found') %>.</p>
    </div>
  {{/if}}
</div>
